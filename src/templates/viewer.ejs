<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LLM Debugger</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Mono:wght@400;500&family=IBM+Plex+Sans:wght@400;500;600&family=Sora:wght@500;600;700&display=swap" rel="stylesheet">
    <style>
        :root {
            color-scheme: light;
            --bg: #f7f8fa;
            --bg-strong: #eef1f4;
            --panel: #ffffff;
            --panel-alt: #f6f8fa;
            --ink: #1d1f23;
            --muted: #5d6670;
            --accent: #10a37f;
            --accent-strong: #0f8a6b;
            --stroke: #e2e6ea;
            --success: #1a7f4a;
            --danger: #b3261e;
            --shadow: 0 20px 50px rgba(15, 23, 42, 0.08);
            --shadow-soft: 0 10px 26px rgba(15, 23, 42, 0.08);
            --focus: rgba(16, 163, 127, 0.22);
        }

        * {
            box-sizing: border-box;
        }

        body {
            margin: 0;
            font-family: "IBM Plex Sans", "Helvetica Neue", sans-serif;
            background:
                radial-gradient(1200px 520px at 12% -10%, rgba(16, 163, 127, 0.12), transparent 60%),
                radial-gradient(1000px 500px at 95% -20%, rgba(15, 23, 42, 0.08), transparent 60%),
                linear-gradient(180deg, var(--bg) 0%, #f1f3f6 100%);
            color: var(--ink);
        }

        .page {
            min-height: 100vh;
            padding: 24px 18px 64px;
        }

        .shell {
            max-width: 1120px;
            margin: 0 auto;
        }

        .hero {
            display: grid;
            gap: 16px;
            padding: 20px 22px;
            background: rgba(255, 255, 255, 0.92);
            border-radius: 20px;
            border: 1px solid var(--stroke);
            box-shadow: var(--shadow-soft);
            position: relative;
            overflow: hidden;
            animation: none;
        }

        .hero::after {
            content: "";
            position: absolute;
            inset: 0;
            background: linear-gradient(120deg, rgba(16, 163, 127, 0.08), transparent 70%);
            pointer-events: none;
        }

        .title-block {
            display: grid;
            gap: 4px;
            position: relative;
            z-index: 1;
        }

        .eyebrow {
            text-transform: uppercase;
            letter-spacing: 0.16em;
            font-size: 0.68rem;
            color: var(--muted);
            margin-bottom: 2px;
        }

        .title {
            font-family: "Sora", "Helvetica Neue", sans-serif;
            font-size: clamp(1.4rem, 1.4vw + 1.2rem, 2rem);
            margin: 0;
        }

        .subtitle {
            margin: 0;
            color: var(--muted);
            line-height: 1.4;
            font-size: 0.95rem;
        }

        .controls {
            display: grid;
            grid-template-columns: minmax(0, 1fr) auto;
            gap: 16px;
            align-items: start;
            padding: 16px;
            border-radius: 18px;
            border: 1px solid var(--stroke);
            background: rgba(246, 248, 250, 0.92);
            box-shadow: var(--shadow-soft);
            position: relative;
            z-index: 1;
        }

        .filters-block {
            display: grid;
            gap: 12px;
        }

        .filters-summary {
            list-style: none;
            display: flex;
            flex-wrap: wrap;
            align-items: center;
            justify-content: flex-start;
            gap: 10px;
            padding: 10px 12px;
            border-radius: 14px;
            border: 1px solid var(--stroke);
            background: #fff;
            cursor: pointer;
            transition: transform 160ms ease, box-shadow 160ms ease, border-color 160ms ease;
        }

        .filters-summary::-webkit-details-marker {
            display: none;
        }

        .filters-summary::marker {
            content: "";
        }

        .filters-summary::after {
            content: "+";
            font-size: 0.9rem;
            color: var(--muted);
            font-weight: 700;
            margin-left: 8px;
        }

        .filters-block[open] .filters-summary::after {
            content: "-";
        }

        .filters-summary:hover {
            border-color: rgba(16, 163, 127, 0.35);
            box-shadow: 0 8px 18px rgba(16, 163, 127, 0.16);
            transform: translateY(-1px);
        }

        .filters-summary:focus-visible {
            outline: 3px solid var(--focus);
            outline-offset: 2px;
        }

        .filters-body {
            display: grid;
            gap: 12px;
        }

        .filters-actions {
            display: flex;
            justify-content: flex-end;
        }

        .filters-title {
            font-size: 0.7rem;
            text-transform: uppercase;
            letter-spacing: 0.14em;
            color: var(--muted);
            font-weight: 600;
        }

        .filters-meta {
            font-size: 0.78rem;
            color: var(--muted);
        }

        .filters-summary .filters-meta {
            margin-left: auto;
        }

        .filters {
            display: grid;
            grid-template-columns: repeat(3, minmax(160px, 1fr));
            gap: 12px;
        }

        .filter-chips {
            display: flex;
            flex-wrap: wrap;
            gap: 8px;
            align-items: center;
        }

        .chip {
            border: 1px solid var(--stroke);
            background: #fff;
            color: var(--muted);
            border-radius: 999px;
            padding: 6px 10px;
            font-size: 0.75rem;
            font-weight: 600;
            display: inline-flex;
            align-items: center;
            gap: 8px;
            cursor: pointer;
            transition: transform 160ms ease, box-shadow 160ms ease, color 160ms ease;
        }

        .chip strong {
            color: var(--ink);
            font-weight: 700;
        }

        .chip:hover {
            color: var(--accent-strong);
            box-shadow: 0 8px 18px rgba(16, 163, 127, 0.18);
            transform: translateY(-1px);
        }

        .chip-empty {
            border-style: dashed;
            background: transparent;
            cursor: default;
            color: var(--muted);
        }

        .chip-remove {
            font-size: 0.7rem;
            text-transform: uppercase;
            letter-spacing: 0.08em;
            color: var(--muted);
        }

        .control-actions {
            display: flex;
            flex-wrap: wrap;
            gap: 10px;
            align-items: center;
            justify-content: flex-end;
            align-self: start;
        }

        .field {
            display: grid;
            gap: 6px;
            padding: 10px 12px;
            border-radius: 14px;
            border: 1px solid var(--stroke);
            background: #fff;
            transition: border-color 180ms ease, box-shadow 180ms ease;
            min-height: 54px;
        }

        .field:focus-within {
            border-color: rgba(16, 163, 127, 0.4);
            box-shadow: 0 0 0 3px rgba(16, 163, 127, 0.14);
        }

        .field-inline {
            display: flex;
            align-items: center;
            gap: 8px;
        }

        label {
            font-size: 0.68rem;
            color: var(--muted);
            text-transform: uppercase;
            letter-spacing: 0.1em;
        }

        input,
        select {
            border: none;
            background: transparent;
            font-size: 0.95rem;
            font-family: "IBM Plex Sans", sans-serif;
            color: var(--ink);
            outline: none;
            caret-color: var(--accent);
        }

        input::placeholder {
            color: rgba(93, 102, 112, 0.6);
        }

        input[type="number"] {
            width: 84px;
        }

        input[type="text"] {
            width: 100%;
            min-width: 0;
        }

        .btn {
            border: none;
            border-radius: 14px;
            padding: 10px 16px;
            font-size: 0.9rem;
            font-weight: 600;
            background: var(--accent);
            color: #fff;
            cursor: pointer;
            transition: transform 180ms ease, box-shadow 180ms ease;
            box-shadow: 0 10px 24px rgba(16, 163, 127, 0.28);
            min-height: 44px;
            justify-self: end;
            align-self: end;
            padding-inline: 20px;
        }

        .btn.secondary {
            background: #fff;
            color: var(--ink);
            border: 1px solid var(--stroke);
            box-shadow: none;
        }

        .btn:hover {
            transform: translateY(-1px);
            box-shadow: 0 16px 32px rgba(16, 163, 127, 0.32);
        }

        .btn:disabled {
            opacity: 0.6;
            cursor: not-allowed;
            transform: none;
            box-shadow: none;
        }

        .logs {
            margin-top: 28px;
            display: grid;
            gap: 16px;
        }

        .empty {
            padding: 48px 24px;
            background: var(--panel);
            border-radius: 22px;
            border: 1px dashed var(--stroke);
            text-align: center;
            color: var(--muted);
        }

        .log-card {
            display: grid;
            gap: 14px;
            padding: 20px 22px;
            padding-right: 60px;
            border-radius: 20px;
            border: 1px solid var(--stroke);
            background: var(--panel);
            text-decoration: none;
            color: inherit;
            position: relative;
            overflow: hidden;
            animation: none;
            cursor: pointer;
            outline: none;
            transition: transform 160ms ease, box-shadow 160ms ease, border-color 160ms ease;
        }

        .log-card::before {
            content: "";
            position: absolute;
            left: 0;
            top: 16px;
            bottom: 16px;
            width: 4px;
            border-radius: 999px;
            background: rgba(16, 163, 127, 0.2);
        }

        .log-card.method-get::before { background: rgba(26, 127, 74, 0.5); }
        .log-card.method-post::before { background: rgba(22, 98, 166, 0.5); }
        .log-card.method-put::before { background: rgba(200, 120, 28, 0.5); }
        .log-card.method-delete::before { background: rgba(179, 38, 30, 0.5); }
        .log-card.method-patch::before { background: rgba(96, 74, 155, 0.5); }

        .log-card.is-selected {
            border-color: rgba(16, 163, 127, 0.5);
            box-shadow: 0 14px 28px rgba(16, 163, 127, 0.18);
            background: linear-gradient(135deg, rgba(16, 163, 127, 0.12), rgba(16, 163, 127, 0.02) 65%), var(--panel);
        }

        .log-card.is-selected::after {
            opacity: 0.6;
        }

        .compare-check {
            position: absolute;
            top: 16px;
            left: 16px;
            width: 28px;
            height: 28px;
            border-radius: 10px;
            border: 1px solid var(--stroke);
            background: #fff;
            display: none;
            align-items: center;
            justify-content: center;
            z-index: 2;
            transition: border-color 160ms ease, box-shadow 160ms ease;
        }

        .compare-check input {
            position: absolute;
            opacity: 0;
            pointer-events: none;
        }

        .compare-indicator {
            width: 14px;
            height: 14px;
            border-radius: 4px;
            border: 2px solid var(--accent);
            background: transparent;
            transition: transform 160ms ease, background 160ms ease, box-shadow 160ms ease;
        }

        .compare-check input:checked + .compare-indicator {
            background: var(--accent);
            box-shadow: 0 0 0 4px rgba(16, 163, 127, 0.16);
            transform: scale(0.95);
        }

        .compare-check.is-disabled {
            opacity: 0.5;
            cursor: not-allowed;
        }

        .overflow {
            position: absolute;
            top: 14px;
            right: 14px;
            z-index: 3;
        }

        .overflow-trigger {
            width: 32px;
            height: 32px;
            border-radius: 10px;
            border: 1px solid var(--stroke);
            background: #fff;
            color: var(--ink);
            display: inline-flex;
            align-items: center;
            justify-content: center;
            font-size: 1.1rem;
            font-weight: 700;
            cursor: pointer;
            transition: transform 160ms ease, box-shadow 160ms ease, border-color 160ms ease;
        }

        .overflow-trigger:hover {
            transform: translateY(-1px);
            box-shadow: 0 8px 18px rgba(16, 163, 127, 0.18);
            border-color: rgba(16, 163, 127, 0.35);
        }

        .overflow-panel {
            position: absolute;
            top: 40px;
            right: 0;
            min-width: 140px;
            padding: 6px;
            border-radius: 14px;
            border: 1px solid var(--stroke);
            background: #fff;
            box-shadow: var(--shadow-soft);
            display: none;
        }

        .overflow.is-open .overflow-panel {
            display: grid;
            gap: 4px;
        }

        .overflow-item {
            display: block;
            width: 100%;
            border: none;
            background: transparent;
            text-align: left;
            padding: 8px 10px;
            border-radius: 10px;
            font-size: 0.85rem;
            font-weight: 600;
            color: var(--ink);
            cursor: pointer;
            font-family: inherit;
            text-decoration: none;
        }

        .overflow-item:hover {
            background: var(--panel-alt);
        }

        .overflow-item.is-danger {
            color: var(--danger);
        }

        .overflow.is-muted {
            position: relative;
            top: auto;
            right: auto;
        }

        .overflow.is-muted .overflow-trigger {
            width: 26px;
            height: 26px;
            border-color: transparent;
            background: transparent;
            color: var(--muted);
            font-size: 0.95rem;
            font-weight: 600;
            box-shadow: none;
            opacity: 0.6;
        }

        .overflow.is-muted .overflow-trigger:hover {
            transform: none;
            box-shadow: none;
            border-color: rgba(93, 102, 112, 0.3);
            background: rgba(255, 255, 255, 0.7);
            opacity: 0.9;
        }

        .overflow.is-muted .overflow-panel {
            top: 32px;
        }

        .hidden-entry {
            display: flex;
            align-items: center;
            justify-content: space-between;
            gap: 12px;
            padding: 6px 8px;
            border-radius: 14px;
            border: 1px dashed rgba(93, 102, 112, 0.25);
            color: var(--muted);
            background: rgba(255, 255, 255, 0.55);
        }

        .hidden-entry .hidden-note {
            padding: 0;
            margin: 0;
        }

        .compare-bar {
            display: none;
            align-items: center;
            justify-content: space-between;
            gap: 16px;
            padding: 16px 20px;
            border-radius: 20px;
            border: 1px solid var(--stroke);
            background: rgba(255, 255, 255, 0.95);
            box-shadow: var(--shadow-soft);
            margin-top: 18px;
            position: sticky;
            top: 18px;
            z-index: 5;
            backdrop-filter: blur(6px);
        }

        .compare-bar.is-active {
            display: flex;
        }

        .compare-title {
            font-weight: 600;
            margin-bottom: 4px;
        }

        .compare-subtitle {
            font-size: 0.85rem;
            color: var(--muted);
        }

        .log-card:focus-visible {
            box-shadow: 0 0 0 3px var(--focus);
            border-color: rgba(16, 163, 127, 0.4);
        }

        .log-card::after {
            content: "";
            position: absolute;
            inset: 0;
            background: linear-gradient(120deg, rgba(16, 163, 127, 0.08), transparent 55%);
            opacity: 0;
            transition: opacity 200ms ease;
        }

        .log-card:hover::after {
            opacity: 1;
        }

        .log-card:hover {
            transform: translateY(-2px);
            box-shadow: var(--shadow-soft);
            border-color: rgba(16, 163, 127, 0.2);
        }

        .log-top,
        .log-meta {
            display: flex;
            flex-wrap: wrap;
            gap: 10px 14px;
            align-items: center;
            position: relative;
            z-index: 1;
            font-variant-numeric: tabular-nums;
        }

        .method {
            font-family: "Sora", sans-serif;
            font-weight: 700;
            font-size: 0.72rem;
            text-transform: uppercase;
            letter-spacing: 0.12em;
            padding: 6px 10px;
            border-radius: 999px;
            background: var(--bg-strong);
            color: var(--muted);
        }

        .method.get { background: rgba(26, 127, 74, 0.12); color: #13633b; }
        .method.post { background: rgba(22, 98, 166, 0.12); color: #1c4a7d; }
        .method.put { background: rgba(200, 120, 28, 0.12); color: #7a4d16; }
        .method.delete { background: rgba(179, 38, 30, 0.12); color: #7e1f18; }
        .method.patch { background: rgba(96, 74, 155, 0.12); color: #4a3a8a; }

        .url {
            font-family: "IBM Plex Mono", monospace;
            font-size: 0.85rem;
            color: var(--ink);
            font-weight: 600;
            max-width: min(620px, 100%);
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
        }

        .pill {
            padding: 5px 10px;
            border-radius: 999px;
            font-size: 0.78rem;
            background: var(--panel-alt);
            border: 1px solid var(--stroke);
            color: var(--muted);
            white-space: nowrap;
            line-height: 1;
        }

        .pill-link {
            text-decoration: none;
            color: var(--muted);
            transition: transform 180ms ease, box-shadow 180ms ease, color 180ms ease;
        }

        .pill-link:hover {
            color: var(--accent-strong);
            box-shadow: 0 8px 18px rgba(16, 163, 127, 0.2);
            transform: translateY(-1px);
        }

        .pill.status.ok {
            color: var(--success);
            border-color: rgba(47, 138, 74, 0.3);
            background: rgba(47, 138, 74, 0.08);
        }

        .pill.status.bad {
            color: var(--danger);
            border-color: rgba(178, 58, 58, 0.3);
            background: rgba(178, 58, 58, 0.08);
        }

        .muted {
            color: var(--muted);
        }

        .hidden-note {
            font-size: 0.78rem;
            color: var(--muted);
            font-style: italic;
            padding: 6px 4px;
        }

        .btn:focus-visible,
        .pill-link:focus-visible {
            outline: none;
            box-shadow: 0 0 0 3px var(--focus);
        }

        body.compare-active .compare-check {
            display: flex;
        }

        body.compare-active .log-card {
            padding-left: 64px;
        }

        @media (prefers-reduced-motion: reduce) {
            * {
                animation: none !important;
                transition: none !important;
            }
        }

        @media (max-width: 720px) {
            .hero {
                padding: 20px;
            }

            .controls {
                grid-template-columns: 1fr;
            }

            .control-actions {
                justify-content: stretch;
                width: 100%;
            }

            .filters-summary {
                align-items: flex-start;
            }

            .btn {
                width: 100%;
                justify-self: stretch;
            }

            .compare-bar {
                flex-direction: column;
                align-items: stretch;
            }

            .log-top {
                align-items: flex-start;
            }

            .url {
                max-width: 100%;
            }

        }

        @media (max-width: 980px) {
            .controls {
                grid-template-columns: 1fr;
            }

            .filters {
                grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
            }

            .control-actions {
                justify-content: stretch;
            }
        }
    </style>
</head>
<body>
    <div class="page">
        <div class="shell">
            <% const baseUrls = (baseUrlFilters || []).filter(Boolean); %>
            <% const aliasFiltersList = (aliasFilters || []).filter(Boolean); %>
            <% const methods = (methodFilters || []).filter(Boolean); %>
            <% const activeFilters = baseUrls.length + aliasFiltersList.length + methods.length; %>
            <% const visibleLogs = logs.reduce((count, log) => count + (log._hidden ? 0 : 1), 0); %>
            <% const hiddenLogs = logs.length - visibleLogs; %>
            <% const filterSummary = activeFilters ? `${activeFilters} active` : 'none'; %>
            <% const filterSummaryMeta = `${filterSummary} / ${visibleLogs} shown`; %>
            <header class="hero">
                <div class="title-block">
                    <div class="eyebrow">LLM Debugger</div>
                    <h1 class="title">Requests</h1>
                </div>
                <div class="controls">
                    <details class="filters-block" id="filters-block">
                        <summary class="filters-summary">
                            <span class="filters-title">Filters</span>
                            <span class="filters-meta"><%= filterSummaryMeta %></span>
                        </summary>
                        <div class="filters-body">
                            <div class="filters">
                                <div class="field">
                                    <label for="limit">Show</label>
                                    <div class="field-inline">
                                        <input type="number" id="limit" value="<%= limit %>" min="1" max="100">
                                        <span class="muted">entries</span>
                                    </div>
                                </div>
                                <div class="field">
                                    <label for="baseUrl">Base URL</label>
                                    <input type="text" id="baseUrl" placeholder="Type host e.g. api.poe.com" value="<%= baseUrls.join(',') %>">
                                </div>
                                <div class="field">
                                    <label for="alias">Alias</label>
                                    <input type="text" id="alias" placeholder="Type alias e.g. openai" value="<%= aliasFiltersList.join(',') %>">
                                </div>
                                <div class="field">
                                    <label for="method">Method</label>
                                    <input type="text" id="method" placeholder="Type something e.g. GET, POST" value="<%= methods.join(',') %>">
                                </div>
                            </div>
                            <div class="filter-chips" aria-live="polite">
                                <% if (!activeFilters) { %>
                                    <span class="chip chip-empty">No filters applied</span>
                                <% } %>
                                <% baseUrls.forEach((value) => { %>
                                    <button class="chip" type="button" data-filter="baseUrl" data-value="<%= value %>" aria-label="Remove base URL filter <%= value %>">
                                        Base URL: <strong><%= value %></strong>
                                        <span class="chip-remove">remove</span>
                                    </button>
                                <% }) %>
                                <% aliasFiltersList.forEach((value) => { %>
                                    <button class="chip" type="button" data-filter="alias" data-value="<%= value %>" aria-label="Remove alias filter <%= value %>">
                                        Alias: <strong><%= value %></strong>
                                        <span class="chip-remove">remove</span>
                                    </button>
                                <% }) %>
                                <% methods.forEach((value) => { %>
                                    <button class="chip" type="button" data-filter="method" data-value="<%= value %>" aria-label="Remove method filter <%= value %>">
                                        Method: <strong><%= value %></strong>
                                        <span class="chip-remove">remove</span>
                                    </button>
                                <% }) %>
                            </div>
                            <div class="filters-actions">
                                <button class="btn secondary" id="clear-filters" type="button" <%= activeFilters ? '' : 'disabled' %>>Clear filters</button>
                            </div>
                        </div>
                    </details>
                    <div class="control-actions">
                        <button class="btn secondary" id="compare-toggle" type="button">Compare</button>
                        <button class="btn" onclick="refresh()" type="button">Refresh</button>
                    </div>
                </div>
            </header>

            <div class="compare-bar" id="compare-bar" aria-live="polite">
                <div>
                    <div class="compare-title">Compare mode</div>
                    <div class="compare-subtitle" id="compare-subtitle">Select 2-3 logs to compare.</div>
                </div>
                <button class="btn" id="compare-submit" type="button" disabled>Compare Selected</button>
            </div>

            <section class="logs">
                <% if (logs.length === 0) { %>
                    <div class="empty">No requests yet. Make a call through the proxy to populate the ledger.</div>
                <% } else { %>
                    <% const backParams = new URLSearchParams(); %>
                    <% backParams.set('limit', limit); %>
                    <% if ((baseUrlFilters || []).length) { backParams.set('baseUrl', baseUrlFilters.join(',')); } %>
                    <% if (aliasFiltersList.length) { backParams.set('alias', aliasFiltersList.join(',')); } %>
                    <% if ((methodFilters || []).length) { backParams.set('method', methodFilters.join(',')); } %>
                    <% const backQuery = backParams.toString() ? `?${backParams.toString()}` : ''; %>
                    <% const hiddenBackParams = new URLSearchParams(backParams.toString()); %>
                    <% hiddenBackParams.set('reveal', '1'); %>
                    <% const hiddenBackQuery = hiddenBackParams.toString() ? `?${hiddenBackParams.toString()}` : ''; %>
                    <% function buildBaseUrlLink(baseUrl) { %>
                        <% const params = new URLSearchParams(backParams.toString()); %>
                        <% const values = (baseUrlFilters || []).slice(); %>
                        <% if (baseUrl && !values.includes(baseUrl)) { values.push(baseUrl); } %>
                        <% if (values.length) { params.set('baseUrl', values.join(',')); } %>
                        <% return params.toString() ? `?${params.toString()}` : ''; %>
                    <% } %>
                    <% function buildAliasLink(alias) { %>
                        <% const params = new URLSearchParams(backParams.toString()); %>
                        <% const values = aliasFiltersList.slice(); %>
                        <% if (alias && !values.includes(alias)) { values.push(alias); } %>
                        <% if (values.length) { params.set('alias', values.join(',')); } %>
                        <% return params.toString() ? `?${params.toString()}` : ''; %>
                    <% } %>
                    <% logs.forEach(function(log, index) { %>
                        <% const providerName = log.provider || 'unknown'; %>
                        <% if (log._hidden) { %>
                            <div class="hidden-entry"
                                data-provider="<%= log._viewer_provider || providerName %>"
                                data-filename="<%= log._viewer_file || '' %>"
                                data-hidden="true">
                                <div class="hidden-note">Hidden: <%= log._path %></div>
                                <div class="overflow is-muted">
                                    <button class="overflow-trigger" type="button" aria-haspopup="true" aria-expanded="false" aria-label="Open hidden log actions">...</button>
                                    <div class="overflow-panel" role="menu">
                                        <a class="overflow-item" role="menuitem" href="/__viewer__/<%= encodeURIComponent(log._viewer_provider || providerName) %>/<%= encodeURIComponent(log._viewer_file || '') %><%= hiddenBackQuery %>">Open</a>
                                        <button class="overflow-item is-danger" type="button" role="menuitem">Delete</button>
                                    </div>
                                </div>
                            </div>
                        <% } else { %>
                            <% const methodLabel = log.request.method || 'UNKNOWN'; %>
                            <% const methodClass = methodLabel.toLowerCase(); %>
                            <% const appName = log.request.headers['x-title'] || log.request.headers['http-referer'] || log.request.headers['X-Title'] || log.request.headers['HTTP-Referer'] || log.request.headers['referer'] || log.request.headers['Referer']; %>
                            <% const baseUrlLabel = log._base_url; %>
                            <% const aliasLabel = log._alias; %>
                            <% const baseUrlDisplay = baseUrlLabel; %>
                            <% const baseUrlFilterValue = baseUrlLabel; %>
                            <% const providerLabel = providerName; %>
                            <% const appLabel = appName; %>
                            <% const normalizedBaseUrlDisplay = (baseUrlDisplay || '').toLowerCase(); %>
                            <% const normalizedAlias = (aliasLabel || '').toLowerCase(); %>
                            <% const normalizedProvider = (providerLabel || '').toLowerCase(); %>
                            <% const normalizedApp = (appLabel || '').toLowerCase(); %>
                            <article class="log-card method-<%= methodClass %>"
                                style="animation-delay: <%= index * 45 %>ms"
                                data-href="/__viewer__/<%= encodeURIComponent(log._viewer_provider || providerName) %>/<%= encodeURIComponent(log._viewer_file || '') %><%= backQuery %>"
                                data-provider="<%= log._viewer_provider || providerName %>"
                                data-filename="<%= log._viewer_file || '' %>"
                                role="link"
                                tabindex="0"
                                aria-label="Open <%= methodLabel %> <%= log.request.url %>">
                                <label class="compare-check">
                                    <input class="compare-input" type="checkbox" aria-label="Select log for compare">
                                    <span class="compare-indicator"></span>
                                </label>
                                <div class="overflow">
                                    <button class="overflow-trigger" type="button" aria-haspopup="true" aria-expanded="false" aria-label="Open log actions">...</button>
                                    <div class="overflow-panel" role="menu">
                                        <button class="overflow-item is-danger" type="button" role="menuitem">Delete</button>
                                    </div>
                                </div>
                                <div class="log-top">
                                    <span class="method <%= methodClass %>"><%= methodLabel %></span>
                                    <span class="url"><%= log.request.url %></span>
                                    <% if (log.response.is_streaming) { %>
                                        <span class="pill">Streaming</span>
                                    <% } %>
                                </div>
                                <div class="log-meta">
                                    <span class="pill status <%= log.response.status < 400 ? 'ok' : 'bad' %>"><%= log.response.status %></span>
                                    <span class="pill"><%= log.duration_ms || 0 %>ms</span>
                                    <span class="pill"><%= new Date(log.timestamp).toLocaleTimeString() %></span>
                                    <% if (baseUrlDisplay) { %>
                                        <a class="pill pill-link" href="<%= buildBaseUrlLink(baseUrlFilterValue) %>"><%= baseUrlDisplay %></a>
                                    <% } %>
                                    <% if (aliasLabel && (!baseUrlDisplay || normalizedAlias !== normalizedBaseUrlDisplay)) { %>
                                        <a class="pill pill-link" href="<%= buildAliasLink(aliasLabel) %>">Alias: <%= aliasLabel %></a>
                                    <% } %>
                                    <% if (providerLabel && (!baseUrlDisplay || normalizedProvider !== normalizedBaseUrlDisplay) && (!aliasLabel || normalizedProvider !== normalizedAlias)) { %>
                                        <span class="pill"><%= providerLabel %></span>
                                    <% } %>
                                    <% if (appLabel && (!baseUrlDisplay || normalizedApp !== normalizedBaseUrlDisplay) && (!providerLabel || normalizedApp !== normalizedProvider) && (!aliasLabel || normalizedApp !== normalizedAlias)) { %>
                                        <span class="pill"><%= appLabel %></span>
                                    <% } %>
                                </div>
                            </article>
                        <% } %>
                    <% }); %>
                <% } %>
            </section>
        </div>
    </div>

    <script>
        function normalizeList(value) {
            return value
                .split(',')
                .map((item) => item.trim())
                .filter(Boolean);
        }

        function refresh() {
            const limitInput = document.getElementById('limit');
            const baseUrlInput = document.getElementById('baseUrl');
            const aliasInput = document.getElementById('alias');
            const methodInput = document.getElementById('method');
            const limit = limitInput ? limitInput.value : '';
            const baseUrl = normalizeList(baseUrlInput ? baseUrlInput.value : '').join(',');
            const alias = normalizeList(aliasInput ? aliasInput.value : '').join(',');
            const method = normalizeList(methodInput ? methodInput.value : '').join(',');
            const params = new URLSearchParams();
            if (limit) {
                params.set('limit', limit);
            }
            if (baseUrl) {
                params.set('baseUrl', baseUrl);
            }
            if (alias) {
                params.set('alias', alias);
            }
            if (method) {
                params.set('method', method);
            }
            window.location.href = '/__viewer__?' + params.toString();
        }

        function clearFilters() {
            const baseUrlInput = document.getElementById('baseUrl');
            const aliasInput = document.getElementById('alias');
            const methodInput = document.getElementById('method');
            if (baseUrlInput) baseUrlInput.value = '';
            if (aliasInput) aliasInput.value = '';
            if (methodInput) methodInput.value = '';
            refresh();
        }

        function removeFilterValue(inputId, valueToRemove) {
            const input = document.getElementById(inputId);
            if (!input) return;
            const nextValues = normalizeList(input.value).filter(
                (item) => item.toLowerCase() !== valueToRemove.toLowerCase()
            );
            input.value = nextValues.join(',');
            refresh();
        }

        document.addEventListener('keydown', (e) => {
            if (e.key === 'r' && (e.metaKey || e.ctrlKey)) {
                e.preventDefault();
                refresh();
            }
        });

        ['limit', 'baseUrl', 'alias', 'method'].forEach((id) => {
            const input = document.getElementById(id);
            if (!input) return;
            input.addEventListener('keydown', (event) => {
                if (event.key === 'Enter') {
                    event.preventDefault();
                    refresh();
                }
            });
        });

        const clearFiltersButton = document.getElementById('clear-filters');
        if (clearFiltersButton) {
            clearFiltersButton.addEventListener('click', () => {
                clearFilters();
            });
        }

        document.querySelectorAll('.chip[data-filter]').forEach((chip) => {
            chip.addEventListener('click', () => {
                const filterType = chip.dataset.filter;
                const filterValue = chip.dataset.value || '';
                if (!filterType) return;
                if (!filterValue) return;
                if (filterType === 'baseUrl') {
                    removeFilterValue('baseUrl', filterValue);
                } else if (filterType === 'alias') {
                    removeFilterValue('alias', filterValue);
                } else if (filterType === 'method') {
                    removeFilterValue('method', filterValue);
                }
            });
        });

        const compareToggle = document.getElementById('compare-toggle');
        const compareBar = document.getElementById('compare-bar');
        const compareSubmit = document.getElementById('compare-submit');
        const compareSubtitle = document.getElementById('compare-subtitle');
        const compareInputs = Array.from(document.querySelectorAll('.compare-input'));
        const compareCards = Array.from(document.querySelectorAll('.log-card[data-provider][data-filename]'));
        const selectionLimit = 3;
        let compareActive = false;

        function getSelectedCards() {
            return compareCards.filter((card) => card.querySelector('.compare-input')?.checked);
        }

        function updateCompareState() {
            if (!compareToggle || !compareBar || !compareSubmit) return;
            document.body.classList.toggle('compare-active', compareActive);
            compareBar.classList.toggle('is-active', compareActive);
            compareToggle.textContent = compareActive ? 'Cancel' : 'Compare';

            const selectedCards = getSelectedCards();
            const selectedCount = selectedCards.length;

            compareSubmit.disabled = selectedCount < 2 || selectedCount > selectionLimit;
            compareSubmit.textContent = selectedCount >= 2
                ? `Compare Selected (${selectedCount})`
                : 'Compare Selected';

            if (compareSubtitle) {
                if (!compareActive) {
                    compareSubtitle.textContent = 'Select 2-3 logs to compare.';
                } else if (selectedCount < 2) {
                    const remaining = 2 - selectedCount;
                    compareSubtitle.textContent = `Selected ${selectedCount}/${selectionLimit}. Select ${remaining} more.`;
                } else {
                    compareSubtitle.textContent = `Selected ${selectedCount}/${selectionLimit}. Ready to compare.`;
                }
            }

            compareCards.forEach((card) => {
                const input = card.querySelector('.compare-input');
                const wrapper = card.querySelector('.compare-check');
                if (!input) return;
                card.classList.toggle('is-selected', input.checked);
                if (compareActive && selectedCount >= selectionLimit && !input.checked) {
                    input.disabled = true;
                    if (wrapper) wrapper.classList.add('is-disabled');
                } else {
                    input.disabled = false;
                    if (wrapper) wrapper.classList.remove('is-disabled');
                }
            });
        }

        function resetCompareSelection() {
            compareInputs.forEach((input) => {
                input.checked = false;
                input.disabled = false;
            });
            compareCards.forEach((card) => card.classList.remove('is-selected'));
        }

        function toggleCardSelection(card) {
            const input = card.querySelector('.compare-input');
            if (!input || input.disabled) return;
            input.checked = !input.checked;
            input.dispatchEvent(new Event('change', { bubbles: true }));
        }

        function buildCompareUrl() {
            const selectedCards = getSelectedCards();
            if (selectedCards.length < 2) return null;

            const selections = selectedCards
                .map((card) => {
                    const provider = card.dataset.provider || 'unknown';
                    const filename = card.dataset.filename || '';
                    if (!filename) return null;
                    return `${encodeURIComponent(provider)}/${encodeURIComponent(filename)}`;
                })
                .filter(Boolean);

            if (selections.length < 2) return null;

            const params = new URLSearchParams();
            params.set('logs', selections.join(','));

            const limit = document.getElementById('limit')?.value || '';
            const baseUrl = normalizeList(document.getElementById('baseUrl')?.value || '').join(',');
            const alias = normalizeList(document.getElementById('alias')?.value || '').join(',');
            const method = normalizeList(document.getElementById('method')?.value || '').join(',');

            if (limit) {
                params.set('limit', limit);
            }
            if (baseUrl) {
                params.set('baseUrl', baseUrl);
            }
            if (alias) {
                params.set('alias', alias);
            }
            if (method) {
                params.set('method', method);
            }

            return '/__viewer__/compare?' + params.toString();
        }

        if (compareToggle && compareBar && compareSubmit) {
            compareToggle.addEventListener('click', () => {
                compareActive = !compareActive;
                if (!compareActive) {
                    resetCompareSelection();
                }
                updateCompareState();
            });

            compareInputs.forEach((input) => {
                input.addEventListener('click', (event) => event.stopPropagation());
                input.addEventListener('change', () => updateCompareState());
            });

            compareSubmit.addEventListener('click', () => {
                const url = buildCompareUrl();
                if (!url) return;
                window.location.href = url;
            });

            updateCompareState();
        }

        const overflowMenus = Array.from(document.querySelectorAll('.overflow'));

        function closeOverflowMenus(except) {
            overflowMenus.forEach((menu) => {
                if (menu === except) return;
                menu.classList.remove('is-open');
                const trigger = menu.querySelector('.overflow-trigger');
                if (trigger) trigger.setAttribute('aria-expanded', 'false');
            });
        }

        async function deleteLogEntry(entry, button) {
            if (!entry) return;
            const provider = entry.dataset.provider || 'unknown';
            const filename = entry.dataset.filename || '';
            if (!filename) return;
            if (!confirm('Delete this log entry? This cannot be undone.')) {
                return;
            }

            const isHidden = entry.dataset.hidden === 'true';
            const deleteUrl = `/__viewer__/${encodeURIComponent(provider)}/${encodeURIComponent(filename)}${isHidden ? '?reveal=1' : ''}`;
            const originalText = button.textContent;
            button.textContent = 'Deleting...';
            button.disabled = true;

            try {
                const response = await fetch(deleteUrl, { method: 'DELETE' });
                if (!response.ok) {
                    const data = await response.json().catch(() => ({}));
                    alert('Failed to delete: ' + (data.error || 'Unknown error'));
                    button.textContent = originalText;
                    button.disabled = false;
                    return;
                }
                window.location.reload();
            } catch (error) {
                alert('Failed to delete: ' + error.message);
                button.textContent = originalText;
                button.disabled = false;
            }
        }

        overflowMenus.forEach((menu) => {
            const trigger = menu.querySelector('.overflow-trigger');
            const deleteButton = menu.querySelector('.overflow-item.is-danger');
            const entry = menu.closest('[data-provider][data-filename]');

            if (trigger) {
                trigger.addEventListener('click', (event) => {
                    event.stopPropagation();
                    const nextState = !menu.classList.contains('is-open');
                    closeOverflowMenus(menu);
                    menu.classList.toggle('is-open', nextState);
                    trigger.setAttribute('aria-expanded', nextState ? 'true' : 'false');
                });
            }

            if (deleteButton) {
                deleteButton.addEventListener('click', (event) => {
                    event.stopPropagation();
                    deleteLogEntry(entry, deleteButton);
                });
            }
        });

        document.addEventListener('click', (event) => {
            if (event.target.closest('.overflow')) return;
            closeOverflowMenus();
        });

        document.addEventListener('keydown', (event) => {
            if (event.key === 'Escape') {
                closeOverflowMenus();
            }
        });

        document.querySelectorAll('.log-card[data-href]').forEach((card) => {
            const href = card.dataset.href;
            if (!href) return;
            const isInteractiveTarget = (event) => event.target.closest('a, .compare-check, .overflow');
            card.addEventListener('click', (event) => {
                if (isInteractiveTarget(event)) return;
                if (compareActive) {
                    toggleCardSelection(card);
                    return;
                }
                window.location.href = href;
            });
            card.addEventListener('keydown', (event) => {
                if (isInteractiveTarget(event)) return;
                if (event.key === 'Enter' || event.key === ' ') {
                    event.preventDefault();
                    if (compareActive) {
                        toggleCardSelection(card);
                        return;
                    }
                    window.location.href = href;
                }
            });
        });
    </script>
</body>
</html>
